{
    "name":"Context Translator",
    "version":"0.1",
    "manifest_version":2,
    "description":"For Public Use",
    "background":{
        "scripts":[
                "jquery-3.1.1.min.js"
            ],
        "persistent": false
    },
    "icons": {
        "16": "img/icon16.png",
        "19": "img/icon19.png",
        "38": "img/icon38.png",
        "48": "img/icon48.png",
        "128": "img/icon128.png"
    },
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*", "<all_urls>"],
        "js": ["jquery-3.1.1.min.js", "content.js"],
        "run_at": "document_end"
      }
    ],
    "permissions":[
        "contextMenus", 
        "tabs",
        "http://*/*",
        "https://*/*"
        ],
    "web_accessible_resources": [
      "img/*.png"
    ]
}
