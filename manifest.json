{
    "name":"How do you feel?",
    "version":"0.1",
    "manifest_version":2,
    "description":"See how your friends feel",
    "background":{
        "scripts":[
                "jquery-3.1.1.min.js",
                "http_requester.js"
            ],
        "persistent": false
    },
    "content_scripts": [
      {
        "matches": ["https://*/*", "http://*/*"],
        "js": ["jquery-3.1.1.min.js", "content.js"],
        "run_at": "document_end"
      }
    ],
    "permissions":[
        "contextMenus", 
        "tabs",
        "http://*/*",
        "https://*/*"
        ],
    "web_accessible_resources": [
      "img/*.png"
    ]
}